    <script>var imageName = '';
        $(document).on('change', '.txtBackgroundImage', function () {
            var fl = $(this);
            var file = $(fl)[0].files[0]

            var form_data = new FormData();
            form_data.append('data', file);
            form_data.append('data', JSON.stringify({ "DocTypeId": "1" }));

            $.ajax({
                url: '/FileUpload/UploadFile',
                cache: false,
                contentType: false,
                processData: false,
                data: form_data,
                type: 'POST',
                success: function (response) {
                    if (response.success) {
                        showToast("!!!", "image uploaded", "success")
                        imageName = response.data.docGuid;
                    } else {
                        alert("error : " + response.message);
                    }
                },
                error: function (error) {
                    console.log('error occured with : ', error);
                }
            });
        })</script>

